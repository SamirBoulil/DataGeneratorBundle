{
    "name": "akeneo/data-generator-bundle",
    "type": "symfony-bundle",
    "description": "Akeneo PIM Bundle to generate data in order to test high volume operations",
    "keywords": ["pim", "akeneo", "ecommerce", "e-commerce", "data", "performances", "csv"],
    "homepage": "http://akeneo.com",
    "license": "OSL-3.0",
    "authors": [
        {
            "name":     "Benoit Jacquemont",
            "email":    "benoit@akeneo.com",
            "homepage": "http://www.akeneo.com/"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-community-dev.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/platform.git"
        }
    ],
    "require": {
        "akeneo/pim-community-dev": "1.2.*",
        "akeneo/pim-community-dev": "dev-master@dev",
        "monolog/monolog": "1.10.*",
        "symfony/icu": "1.1.0",
        "oro/platform": "1.0.0-beta13",
        "knplabs/knp-menu-bundle":"v2.0.0-alpha2",
        "knplabs/knp-menu":"v2.0.0-alpha2",
        "gedmo/doctrine-extensions":"2.3.x-dev@dev",
        "fzaninotto/faker": "1.4.*"
    },
    "require-dev": {
        "phpspec/phpspec": "2.0.*@dev"
    },
    "autoload": {
        "psr-0": { "Pim\\Bundle\\DataGeneratorBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "target-dir": "Pim/Bundle/DataGeneratorBundle"
}
